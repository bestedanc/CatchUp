<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.catchup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CatchUp">
        <activity android:name=".activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.DiyetisyeneSorActivity" />
        <activity android:name=".activities.WorkoutActivity" />
        <activity android:name=".activities.WaterActivity" />
        <activity android:name=".activities.DietListActivity" />
        <activity android:name=".activities.HomeActivity" />
        <activity android:name=".activities.WelcomeActivity" />
        <activity
            android:name=".activities.LoginActivity"
            android:parentActivityName=".activities.WelcomeActivity" />
        <activity
            android:name=".activities.RegisterActivity"
            android:parentActivityName=".activities.WelcomeActivity" />
    </application>

</manifest>